<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net472</TargetFramework>
    <LangVersion>11.0</LangVersion>         <!-- Enforces compatible C# version -->
    <Nullable>disable</Nullable>           <!-- Disable nullability to avoid C# 8+ error -->
    <ImplicitUsings>disable</ImplicitUsings> <!-- Also disable implicit using for compatibility -->
  </PropertyGroup>

</Project>
